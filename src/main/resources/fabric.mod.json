{
  "schemaVersion": 1,
  "id": "chestesp",
  "version": "${version}",
  "name": "ChestESP",
  "description": "Highlights nearby chests so you can see them through walls. Based on a Wurst Client feature of the same name.",
  "authors": [
    "Alexander01998"
  ],
  "contact": {
    "homepage": "https://www.wimods.net/chestesp/modmenu-homepage/",
    "sources": "https://www.wimods.net/chestesp/modmenu-sources/",
    "issues": "https://www.wimods.net/chestesp/modmenu-issues/"
  },
  "license": "GPL-3.0-or-later",
  "icon": "assets/chestesp/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.wimods.chestesp.ChestEspModInitializer"
    ],
    "modmenu": [
      "net.wimods.chestesp.ChestEspModMenuApi"
    ]
  },
  "mixins": [
    "chestesp.mixins.json"
  ],
  "accessWidener": "chestesp.accesswidener",
  "depends": {
    "fabricloader": ">=0.17.3",
    "fabric-api": ">=0.140.0",
    "modmenu": "^17.0.0-alpha.1",
    "cloth-config": "^21.11.150",
    "minecraft": "~1.21.11-beta.1",
    "java": ">=21"
  },
  "suggests": {
    "wi_zoom": "*",
    "cameranoclip": "*",
    "mo_glass": "*"
  },
  "breaks": {
    "wurst": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.donate": "https://www.wimods.net/chestesp/modmenu-donate/",
        "modmenu.twitter": "https://www.wimods.net/chestesp/modmenu-twitter/",
        "Forum": "https://www.wimods.net/chestesp/modmenu-forum/"
      }
    }
  }
}

{
  "schemaVersion": 1,
  "id": "chestesp",
  "version": "${version}",
  "name": "ChestESP",
  "description": "Highlights nearby chests so you can see them through walls. Based on a Wurst Client feature of the same name.",
  "authors": [
    "Alexander01998"
  ],
  "contact": {
    "homepage": "https://www.wimods.net/chestesp/?utm_source=ChestESP&utm_medium=ModMenu&utm_content=ChestESP+ModMenu+website+link",
    "sources": "https://github.com/Wurst-Imperium/ChestESP",
    "issues": "https://github.com/Wurst-Imperium/ChestESP/issues",
    "email": "contact.wurstimperium@gmail.com",
    "twitter": "https://twitter.com/Wurst_Imperium"
  },
  "license": "GPL-3.0-or-later",
  "icon": "assets/chestesp/icon.png",
  "environment": "client",
  "entrypoints": {
    "main": [
      "net.wimods.chestesp.ChestEspModInitializer",
      "net.wimods.chestesp.test.ChestESPTestClient"
    ],
    "modmenu": [
      "net.wimods.chestesp.ChestEspModMenuApi"
    ]
  },
  "mixins": [
    "chestesp.mixins.json"
  ],
  "accessWidener": "chestesp.accesswidener",
  "depends": {
    "fabricloader": ">=0.16.10",
    "fabric-api": ">=0.119.1",
    "modmenu": "^14.0.0-beta.2",
    "cloth-config": "^17.0.144",
    "minecraft": "~1.21.5-beta.3",
    "java": ">=21"
  },
  "suggests": {
    "wi_zoom": "*",
    "cameranoclip": "*",
    "mo_glass": "*"
  },
  "breaks": {
    "wurst": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "Donate": "https://www.wurstclient.net/donate/?utm_source=ChestESP&utm_medium=ModMenu&utm_content=ChestESP+ModMenu+donate+link"
      }
    }
  }
}
